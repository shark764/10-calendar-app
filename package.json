{
  "name": "calendar-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "full-check": "yarn run prettify && yarn run typescript",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "prettify": "yarn run format && yarn run lint:fix",
    "typescript": "tsc --noEmit",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "date-fns": "^2.29.1",
    "date-fns-tz": "^1.3.6",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-avatar": "^5.0.1",
    "react-big-calendar": "^0.40.1",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.0.0",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.3",
    "sweetalert2": "^11.4.17"
  },
  "devDependencies": {
    "@2600hz/commio-native-utilities": "^0.5.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.12",
    "@types/react-big-calendar": "^0.38.0",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
